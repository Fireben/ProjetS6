<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'
	xmlns:npui="urn:import:educatus.client.ui">

	<g:HTMLPanel>
	   	<g:VerticalPanel ui:field="profilVerticalPanel">
			   <npui:ProfilSummary ui:field="profilSummary"></npui:ProfilSummary>
			   <npui:ProfilNewsfeed ui:field="profilNewsfeed"></npui:ProfilNewsfeed>
			   <npui:UserStatisticsWidget ui:field="userStatisticsWidget"></npui:UserStatisticsWidget>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
