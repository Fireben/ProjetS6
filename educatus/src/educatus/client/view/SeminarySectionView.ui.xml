<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:my="urn:import:educatus.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:with field='res' type='educatus.resources.Resources'/>
	
	
	<ui:style>
	 	@external .gwt-b;
		.gwt-b {
		width: 48px;
		height: 48px;
        background: none;
        border: none;
    }
    
	<!--     @external .test; -->
	</ui:style>

	<g:HTMLPanel>
		
		<div id="art-mainPage">
			<div class="test">
				<g:TextBox ui:field="textBox">	
				</g:TextBox>
				
				<g:PushButton ui:field="firstButton" enabled='true' addStyleNames='{style.gwt-b}'>
				
					<g:upFace image='{res.deleteButton}'/>
					
				</g:PushButton>
				
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
