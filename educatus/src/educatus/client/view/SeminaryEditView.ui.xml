<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<style>

		</style>

		<div id="art-mainPage">
			<form>
				<div class="art-blockcontent">
					<h4>
						<label for="seminaryTitle" ui:field="semTitleLabel">Titre :</label>
						<input type="text" name="seminaryTitle" id="seminaryTitle"
							style="width:25%;" />
						<br/>
						<label for="seminaryDescription">Description :</label>
						<br/>
						<textarea name="seminaryDescription" id="seminaryDescription" style="width:25%; resize:none; margin-left:15px">
							Entrez votre description ici...
						</textarea>
					</h4>
				</div>
				
			</form>
		</div>
		
		<g:HTMLPanel ui:field="confirmPanel"/>
	</g:HTMLPanel>
</ui:UiBinder>
