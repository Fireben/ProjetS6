<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:npui="urn:import:educatus.client.ui">
    <ui:with type="educatus.client.NameTokens" field="nameTokens"></ui:with>
	<g:HTMLPanel height= "100%">
		<div class="wrapper">
			<div class="header">
				<g:FlowPanel ui:field="headerPanel"/>
			</div>
			<div class="content">
				<g:FlowPanel ui:field="mainContentPanel" height= "100%" />
			</div>
			<div class="footer">
				<npui:Footer ui:field="footerPanel" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>